{
  "name": "tempel-des-schreckens",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [{ "module": "es6", "in-source": true }],
  "suffix": ".bs.js",
  "refmt": 3,
  "bs-dependencies": ["@glennsl/bs-json", "bs-fetch", "bucklescript-tea"],
  "bsc-flags": [
    "-bs-super-errors -no-alias-deps",
    "-color",
    "always",
    "-w",
    "-40+6+7+27+32..39+44+45",
    "-w @8"
  ],
  "warnings": {
    "error": "+101"
  },
  "namespace": true
}